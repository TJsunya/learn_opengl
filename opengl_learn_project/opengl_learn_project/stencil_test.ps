#version 330 core
out vec4 FragColor;

in vec2 TexCoords;

uniform sampler2D texture1;
float near =0.1;
float far = 100.0;

float LinearizeDepth(float depth){
	float z = depth * 2.0 - 1.0;
	return (near*far)/(far -z *(far-near))/far;
}


void main()
{    
   FragColor = vec4(1.0, 0.0, 0.0, 1.0);
